# Your Snowflake account identifier
# Format: <orgname>-<accountname>  OR  <accountlocator>.<region>
# Example: xy12345.ap-southeast-1   or   myorg-myaccount
SNOWFLAKE_ACCOUNT=your_account_identifier

# Login credentials
SNOWFLAKE_USER=your_username
SNOWFLAKE_PASSWORD=your_password

# Snowflake objects to use
SNOWFLAKE_DATABASE=BARCLAY_KYC_DB
SNOWFLAKE_SCHEMA=KYC
SNOWFLAKE_WAREHOUSE=COMPUTE_WH

# Table that holds the company registry
# (Created by setup_snowflake.sql)
SNOWFLAKE_TABLE=COMPANY_REGISTRY
